{
  "name": "codasis",
  "displayName": "Codasis",
  "description": "AI-powered code assistance using DeepSeek R1 and other powerful language models",
  "version": "0.0.1",
  "publisher": "augment-ai-replica",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/augment-ai-replica/vscode-extension"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "ai",
    "assistant",
    "deepseek",
    "code-completion",
    "nlp"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "augment-ai.askQuestion",
        "title": "Ask AI Question",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.analyzeCode",
        "title": "Analyze Current Code",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.explainCode",
        "title": "Explain Selected Code",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.generateTests",
        "title": "Generate Tests",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.refactorCode",
        "title": "Refactor Code",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.openChat",
        "title": "Open AI Chat",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.indexWorkspace",
        "title": "Index Workspace for AI Context",
        "category": "Augment AI"
      },
      {
        "command": "augment-ai.smartAnalyze",
        "title": "Smart Code Analysis",
        "category": "Augment AI"
      }
    ],
    "keybindings": [
      {
        "command": "augment-ai.askQuestion",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      },
      {
        "command": "augment-ai.openChat",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c"
      },
      {
        "command": "augment-ai.indexWorkspace",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i"
      },
      {
        "command": "augment-ai.smartAnalyze",
        "key": "ctrl+shift+s",
        "mac": "cmd+shift+s"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "augment-ai.explainCode",
          "when": "editorHasSelection",
          "group": "augment-ai"
        },
        {
          "command": "augment-ai.refactorCode",
          "when": "editorHasSelection",
          "group": "augment-ai"
        },
        {
          "command": "augment-ai.smartAnalyze",
          "when": "editorHasSelection",
          "group": "augment-ai"
        }
      ],
      "explorer/context": [
        {
          "command": "augment-ai.analyzeCode",
          "when": "resourceExtname == .js || resourceExtname == .ts || resourceExtname == .py || resourceExtname == .php",
          "group": "augment-ai"
        }
      ]
    },
    "views": {
      "augment-ai": [
        {
          "id": "augment-ai-chat",
          "name": "AI Chat",
          "type": "webview"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "augment-ai",
          "title": "Augment AI",
          "icon": "$(robot)"
        }
      ]
    },
    "configuration": {
      "title": "Augment AI",
      "properties": {
        "augment-ai.apiUrl": {
          "type": "string",
          "default": "http://localhost:8000/api",
          "description": "Backend API URL"
        },
        "augment-ai.apiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for authentication"
        },
        "augment-ai.preferredModel": {
          "type": "string",
          "enum": ["deepseek-r1", "gpt-4", "claude-3", "gemini-pro"],
          "default": "deepseek-r1",
          "description": "Preferred AI model for responses"
        },
        "augment-ai.autoComplete": {
          "type": "boolean",
          "default": true,
          "description": "Enable auto-completion suggestions"
        },
        "augment-ai.contextLines": {
          "type": "number",
          "default": 50,
          "description": "Number of context lines to send with requests"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/vsce": "^2.15.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
